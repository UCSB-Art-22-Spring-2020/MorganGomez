void colore(){
  
  if(keyPressed){
    
     if (key == 'q') {
      ambientLight(230,150,120); // orange
    }
    
    if (key == 'a') {
      ambientLight(65,180,230); // blue
    }
    
    if (key == 'z') {
      ambientLight(200,130,160); // pink
    }
    
    if (key == 'w') {
      ambientLight(250,200,95); // yellow
    }
    
    if (key == 's') {
      ambientLight(230,85,75); // red
    }
    
    if (key == 'x') {
      ambientLight(75,150,100); // green
    }
    
  }
}
